{
    "name": "betterde/swoole",
    "description": "Swoole server",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George",
            "email": "george@betterde.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "Betterde\\Swoole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Betterde\\Swoole\\Providers\\SwooleServiceProvider"
            ],
            "aliases": {
                "Server": "Betterde\\Swoole\\Facades\\Server"
            }
        }
    }
}
